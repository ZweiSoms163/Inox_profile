{
  "name": "webpack-static-html-pages",
  "version": "1.0.0",
  "description": "Example of a static website assembled by using webpack v4",
  "keywords": [
    "webpack",
    "static",
    "starter",
    "html",
    "pages"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "copy-webpack-plugin": "^6.2.1",
    "jquery": "^3.5.1",
    "normalize.css": "^8.0.1"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --mode development",
    "start:dist": "http-server dist --port ${PORT:-8080}",
    "build": "webpack --config webpack.prod.js --mode production",
    "preview": "npm run build && npm run start:dist",
    "test": "standard"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.0",
    "postcss-loader": "^4.1.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.2.1",
    "svg-spritemap-webpack-plugin": "^3.8.1",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.0"
  },
  "standard": {
    "ignore": [],
    "env": [
      "node",
      "es6"
